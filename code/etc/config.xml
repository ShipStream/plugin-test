<?xml version="1.0"?>
<config>
    <modules>
        <ShipStream_Test>
            <version>0.1</version>
        </ShipStream_Test>
    </modules>
    <default>
        <plugin>
            <ShipStream_Test>
                <whoami>*Your Name Here*</whoami>
                <service_url>http://bot.whatismyipaddress.com</service_url>
                <events>
                    <order>
                        <created>1</created>
                    </order>
                </events>
                <webhooks>1</webhooks>
            </ShipStream_Test>
        </plugin>
    </default>
    <plugin_crontab>
        <jobs>
            <dummy_cron_job>
                <schedule><cron_expr>5 * * * *</cron_expr></schedule><!-- Extended Unix crontab syntax -->
                <run><class>ShipStream_Test_Plugin::dummyCronJob</class></run><!-- {CLASS NAME}::{METHOD NAME} -->
            </dummy_cron_job>
        </jobs>
    </plugin_crontab>
</config>
